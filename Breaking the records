def breakingRecords(scores):
    maxs=0
    mins=0
    max1=scores[0]
    min1=scores[0]
    for i in scores:
        if i>max1:
            max1=i
            maxs=maxs+1
        elif i<min1:
            min1=i
            mins=mins+1
    lst=[]
    lst.append(maxs)
    lst.append(mins)
    return lst
